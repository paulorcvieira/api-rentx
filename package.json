{
  "name": "rentx",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Paulo Vieira",
    "github": "https://github.com/paulorcvieira"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && exit 1"
    }
  },
  "lint-staged": {
    "{clients,server}/**/src/**/*.ts?(x)": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "scripts": {
    "dev": "tsnd --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "typeorm": "tsnd ./node_modules/typeorm/cli"
  },
  "dependencies": {
    "aws-sdk": "^2.874.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "cors": "^2.8.5",
    "csv-parse": "^4.15.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@commitlint/core": "^11.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "commitizen": "^4.2.3",
    "commitlint-config-gitmoji": "^2.1.10",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
